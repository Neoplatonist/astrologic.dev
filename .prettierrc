{
  "plugins": [
    "./node_modules/prettier-plugin-astro"
  ],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ],
  "endOfLine": "lf",
  "printWidth": 80,
  "tabWidth": 2,
  "trailingComma": "es5",
  "singleQuote": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "semi": true,
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "css"
}
